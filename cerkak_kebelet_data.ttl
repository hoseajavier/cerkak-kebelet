:aksara_baris012 a :TeksAksara ;
    rdf:value "\"ꦢꦭꦺꦩꦶꦥꦸꦤ꧀?\"ꦥꦿꦶꦪꦏꦸꦮꦶꦒꦼꦤ꧀ꦠꦶꦔꦺꦴꦪꦏ꧀꧉"^^<http://www.w3.org/1999/02/22-rdf-syntax-ns#XMLLiteral> ;
    lexinfo:script :AksaraJawa ;
    :jumlahKarakter "32"^^xsd:integer .

:translit_baris012 a :Transliterasi ;
    rdf:value "\"Dalêmipún?\" priyå kuwi gênti ngoyak."@jv-Latn ; # 
    :menggunakanAturan "Transliterasi Latin Aksara Jawa" .

:terjemah_baris012 a :Terjemahan ;
    rdf:value "\"Rumahnya?\" pria itu ganti mengejar."@id .

