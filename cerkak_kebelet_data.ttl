    :hasTranslation :terjemah_baris014 ;
    dcterms:identifier "BARIS-014" .

:aksara_baris014 a :TeksAksara ;
    rdf:value "\"ꦲꦺꦴ.....,\"ꦥꦿꦶꦪꦏꦸꦮꦶꦩꦸꦤꦶꦲꦺꦴ'꧉"^^<http://www.w3.org/1999/02/22-rdf-syntax-ns#XMLLiteral> ;
    lexinfo:script :AksaraJawa ;
    :jumlahKarakter "25"^^xsd:integer .

:translit_baris014 a :Transliterasi ;
    rdf:value "\"O.....,\" priyå kuwi muni 'o'."@jv-Latn ; # 
    :menggunakanAturan "Transliterasi Latin Aksara Jawa" .

:terjemah_baris014 a :Terjemahan ;
    rdf:value "\"O.....,\" pria itu berkata 'o'."@id .

