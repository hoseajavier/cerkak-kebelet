    rdf:value "\"ꦠꦸꦭꦸꦁꦲꦒꦸꦁ꧉\""^^<http://www.w3.org/1999/02/22-rdf-syntax-ns#XMLLiteral> ;
    lexinfo:script :AksaraJawa ;
    :jumlahKarakter "13"^^xsd:integer .

:translit_baris013 a :Transliterasi ;
    rdf:value "\"Tulúngagúng.\""@jv-Latn ; # 
    :menggunakanAturan "Transliterasi Latin Aksara Jawa" .

:terjemah_baris013 a :Terjemahan ;
    rdf:value "\"Tulungagung.\""@id .

